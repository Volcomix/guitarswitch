#include "../articulation.ttl.in"

<https://github.com/Volcomix/guitarswitch/plugins/duplicate>
    a lv2:Plugin ;
    doap:name "Duplicate articulation" ;
    doap:license <http://www.gnu.org/licenses/gpl.html> ;
    lv2:project <https://github.com/Volcomix/guitarswitch> ;
    lv2:requiredFeature urid:map ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port GS_ARTICULATION_PORTS() ,
    [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "transpose" ;
        lv2:name "Transpose" ;
        lv2:portProperty lv2:integer ;
        lv2:default 7 ;
        lv2:minimum -24 ;
        lv2:maximum 24 ;
        units:unit units:semitone12TET ;
        lv2:scalePoint [
            rdfs:label "-2 oct" ;
            rdf:value -24
        ] , [
            rdfs:label "-1 oct" ;
            rdf:value -12
        ] , [
            rdfs:label "-7 semi" ;
            rdf:value -7
        ] , [
            rdfs:label "Off" ;
            rdf:value 0
        ] , [
            rdfs:label "+7 semi" ;
            rdf:value 7
        ] , [
            rdfs:label "+1 oct" ;
            rdf:value 12
        ] , [
            rdfs:label "+2 oct" ;
            rdf:value 24
        ]
    ] .
